{
  "manifest_version": 3,
  "name": "Ad-Blocker by Raghav",
  "version": "1.1",
  "description": "Blocks common web, video, and popup ad sources.",
  "permissions": ["declarativeNetRequest", "declarativeNetRequestFeedback", "storage"],
  "host_permissions": ["*://*/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Ad Blocker",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  },
  "icons": {
    "16": "image.png",
    "48": "image.png",
    "128": "image.png"
  }
}

    
  